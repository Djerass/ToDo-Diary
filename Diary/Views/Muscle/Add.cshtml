@model ToDoDiaryWeb.Models.MuscleGroup

@{
    ViewBag.Title = "Adding Muscle Group";
    Layout = "_Layout";
}

<h2>Add muscle group</h2>
<form asp-controller="Muscle" asp-action="Add" method="post">
    <div class="form-group">
        <label  asp-for="Name"></label>
        <input class="form-control" type="text" required="" asp-for="Name"/>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    
</form> 

